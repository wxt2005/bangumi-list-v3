generator client {
  provider = "prisma-client"
  output   = "./generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model BangumiPreference {
  userID    String  @id
  watching  String?
  createdAt DateTime
  updatedAt DateTime
}

model Preference {
  userID    String @id
  common    String
  createdAt DateTime
  updatedAt DateTime
}

model Token {
  userID String
  token  String

  @@id([userID, token])
}

model User {
  id         String @id
  email      String @unique(map: "sqlite_autoindex_user_2")
  password   String
  role       Int
  createdAt DateTime
  updatedAt DateTime
}
